@{
    ViewData["Title"] = "Каталог продукции";
}

<div>
    <h1 class="display-4">Пользователи</h1>
    <div>
        <table>
            <thead>
                <tr>
                    <td> Ид </td>
                    <td> Роль </td>
                    <td> Логин </td>
                    <td> Фамилия </td>
                    <td> Имя </td>
                    <td> Отчество </td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in ViewBag.Users)
                {
                <tr>
                    <td> @item.Id </td>
                    <td> @item.IdRole </td>
                    <td> @item.UserLogin </td>
                    <td> @item.LastName </td>
                    <td> @item.FirstName </td>
                    <td> @item.SecondName </td>

                </tr>
                }
            </tbody>
        </table>
    </div>
</div>
